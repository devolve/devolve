<%= content_for :extra_css do %>

<link  href="http://fonts.googleapis.com/css?family=Rock+Salt:regular" rel="stylesheet" type="text/css" />


<%= stylesheet_link_tag "resend" %>
<% end %>
<div class="wrapper">
<div id="header"><div id="logoname">Devolve</div></div>
<div id="signup">

	<h6 id="a1">Resend unlock instructions</h6>
	
	
	<p id="finish_signup">Please enter your email here, We will send the unlock instructions.</p>


<%= form_for(resource, :as => resource_name, :url => unlock_path(resource_name), :html => { :method => :post }) do |f| %>
  <%= devise_error_messages! %>
<div class="inputRow containerFix">
          <div class="inputName">
  <p><%= f.label :email ,:id => "email_lab"%><br /></div>
		 <div class="inputValue">
	
  <%= f.text_field :email %></p></div></div>

  <p><%= f.submit "Resend unlock instructions" %></p>
<% end %>

<%= render :partial => "devise/shared/links" %>
</div>
</div>